    "@types/node": "^16.18.36",
    "@types/node-fetch": "^2.6.4",
    "@types/node-forge": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "homebridge": "^1.3.5",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "node-forge": "^1.3.1",
    "node-fetch": "2.6.11"
  }
